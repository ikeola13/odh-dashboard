    "tls": false,
    "proxy": [
      {
        "path": "/lm-eval/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9001
    },
    "service": {
      "name": "lm-eval-ui-service",
      "port": 8081
    }
  }
}
