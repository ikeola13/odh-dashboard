{
  "private": true,
  "name": "@odh-dashboard/lm-eval",
  "description": "Language Model Evaluation plugin.",
  "version": "0.0.0",
  "scripts": {
    "update-subtree": "../../scripts/package-subtree.sh lm-eval"
  },
  "subtree": {
    "repo": "https://github.com/trustyai-explainability/trustyai-dashboard.git",
    "src": "",
    "target": "upstream",
