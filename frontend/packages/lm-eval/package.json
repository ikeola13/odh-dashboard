{
  "private": true,
  "name": "@odh-dashboard/lm-eval",
  "description": "Language Model Evaluation plugin.",
  "version": "0.0.0",
  "scripts": {
    "update-subtree": "../../scripts/package-subtree.sh lm-eval"
  },
  "subtree": {
    "repo": "https://github.com/trustyai-explainability/trustyai-dashboard.git",
    "src": "",
    "target": "upstream",
    "commit": "39375301a9ae81ae195428fe218420ad71c1e337"
  },
  "module-federation": {
    "name": "lmEval",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/lm-eval/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9000
    },
    "service": {
      "name": "lm-eval-ui-service",
      "port": 8080
    }
  },
  "exports": {
    "./extensions": "./upstream/frontend/odh/extensions.ts"
  }
}
